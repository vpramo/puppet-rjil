#Template for generating Rsyslog Configuration
#This file is automatically generated by Puppet
#Although editing this file is allowed, it will be 
#overridden by puppet
<% @tmpl_filter.each_pair do |key, value_hash| %>
$template <%= value_hash['template']['name'] %>, "<%= value_hash['template']['expr']%>"
<% %>
<% if value_hash.has_key?('filter') %>
:<%= value_hash['filter']['def'] %>,<%= value_hash['filter']['match']%>, "<%= value_hash['filter']['string']%>"
<% end %>
<% %>
<%= value_hash['output']['local']%>.<%= value_hash['output']['type']%> -?<%= value_hash['template']['name']%>
<% %>
<% end %>
